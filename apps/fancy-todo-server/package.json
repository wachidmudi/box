  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "google-auth-library": "^6.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.3.3",
    "sequelize": "^6.37.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/sequelize": "^4.28.20",
    "dotenv": "^8.2.0",
    "sequelize-cli": "^6.2.0",
    "typescript": "^5.3.3"
  }
